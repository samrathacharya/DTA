    </formulas>
    <formulas>
        <name>PhoneNumberFormula</name>
        <dataType>String</dataType>
        <expression>IF(CONTAINS({!PhoneNumberString}, &quot;B&quot;),
MID({!PhoneNumberString},{!StartOfPhoneNumberForPlus},{!EndOfString}),
MID({!PhoneNumberString},{!StartOfPhoneNumberForOther},{!EndOfString})
)</expression>
    </formulas>
    <formulas>
        <name>PhoneNumberString</name>
        <dataType>String</dataType>
        <expression>MID({!icAgentConsole_incCustomScreenpop},1,{!EndOfString})</expression>
    </formulas>
    <formulas>
        <name>remainingText</name>
        <dataType>String</dataType>
        <expression>MID({!icAgentConsole_incCustomScreenpop},({!EndOfString}+2),LEN({!icAgentConsole_incCustomScreenpop}))</expression>
    </formulas>
    <formulas>
        <name>StartOfPhoneNumberForOther</name>
        <dataType>Number</dataType>
        <expression>FIND(&apos;=&apos;,{!PhoneNumberString}) + 1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>StartOfPhoneNumberForPlus</name>
        <dataType>Number</dataType>
        <expression>FIND(&apos;B&apos;,{!PhoneNumberString}) + 1</expression>
