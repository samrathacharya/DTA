        string backendMessage = m.get('backendMessage');
        if (outputData == null) {
            return new ServiceContractReq();
        }
        ServiceContractReq obj = (ServiceContractReq)JSON.deserialize(outputData, ServiceContractReq.class);
        obj.outputData = outputData;
        obj.backendMessage = backendMessage;

